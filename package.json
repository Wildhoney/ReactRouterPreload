{
  "name": "react-router-preload",
  "version": "0.1.0",
  "description": "Augments React Router v4 to support preloading data before route changes, AJAX cancellation, SSR fetching, and other niceties.",
  "main": "dist/index.js",
  "scripts": {
    "test": "nyc ava",
    "clean": "rimraf .nyc_output node_modules coverage",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wildhoney/ReactRouterPreload.git"
  },
  "keywords": [
    "react",
    "react router",
    "async",
    "data",
    "preload",
    "ajax",
    "cancel"
  ],
  "author": "Adam Timberlake <adam.timberlake@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wildhoney/ReactRouterPreload/issues"
  },
  "homepage": "https://github.com/Wildhoney/ReactRouterPreload#readme",
  "peerDependencies": {
    "history": ">=4.0.0",
    "react-router-config": ">=1.0.0"
  },
  "devDependencies": {
    "ava": "~0.19.1",
    "babel-loader": "~7.0.0",
    "babel-polyfill": "~6.23.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "browser-env": "~3.2.1",
    "delay": "~2.0.0",
    "enzyme": "~3.0.0",
    "enzyme-adapter-react-16": "~1.0.1",
    "history": "~4.7.2",
    "nyc": "~11.1.0",
    "react": "~16.0.0",
    "react-dom": "~16.0.0",
    "react-router-config": "~1.0.0-beta.4",
    "react-router-dom": "~4.1.2",
    "react-test-renderer": "~16.0.0",
    "rimraf": "~2.6.2",
    "sinon": "~2.3.8",
    "starwars": "~1.0.0",
    "webpack": "~3.0.0"
  },
  "ava": {
    "concurrency": 3,
    "failFast": true,
    "files": [
      "./tests/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-polyfill",
      "babel-register",
      "./tests/helpers/browser-env.js",
      "./tests/helpers/enzyme.js"
    ]
  }
}
